<?xml version="1.0" encoding="utf-8"?>
<!--
   Sample data extraction rules file.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules xmlns:android="http://schemas.android.com/apk/res/android">

    <cloud-backup>
        <!-- Include and exclude define what files/databases/shared-prefs get backed up -->
        <include domain="sharedpref" path="."/>
        <include domain="database" path="."/>
        <exclude domain="database" path="temp.db"/>
    </cloud-backup>

    <device-transfer>
        <include domain="sharedpref" path="."/>
        <include domain="database" path="."/>
    </device-transfer>

</data-extraction-rules>
